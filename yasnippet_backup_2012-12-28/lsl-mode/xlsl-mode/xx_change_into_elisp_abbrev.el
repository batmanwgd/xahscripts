(setq  baseDir "~/git/xahscripts/yasnippet_backup_2012-12-28/lsl-mode/xlsl-mode/")

(setq output "")

(defun x-read-lines (φfile-path)
  ""
  (with-temp-buffer
    (insert-file-contents φfile-path)
    (buffer-string)))

(mapc
 (lambda (fname)
   (progn
     (setq output
           (concat
            output "(\"" fname "\"" " \""
            (x-read-lines (concat baseDir fname ".yasnippet"))
            "\" nil :system t)\n"
            ))))
 files)

(insert output)
